package Contructor;

import java.util.Objects;

public class Example {
	private int roll;
	private static  String name;

	public  Example(String name) {
		
		System.out.println("Hàm contructor mặc định" + name);
	}
	
	public Example(int roll) {		
		this(name);
		System.out.println(roll);
	}
	
	

	
	
	
	
	
	
}
