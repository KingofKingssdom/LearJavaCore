package Test;

public class Test {
	public static int CanBacHai(int n) {
		 int res = 0;
	        while (res * res < n) {
	            res++;
	        }
	        
	        // return the largest integer whose 
	        // square is less than or equal to n
	        return res;
	    
	}
	public static void main(String[] args) {
		int x = 4;
		System.out.println(CanBacHai(x));
		
	}
}
