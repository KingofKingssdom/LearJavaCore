package Arrays;

import java.util.Arrays;
import java.util.List;

public class Test {
	public static int timKiemTuyenTinh(int [] array, int x) {
		int n = array.length;
		for(int i = 0; i < n;i++) {
			if(x == array[i]) {
				return i;
			}
		}
		return -1;
	}
	public static void main(String[] args) {
		int [] array = {10,20,30,40,50,60};
		int result = timKiemTuyenTinh(array, 200);
		System.out.println(result);
	}
}
