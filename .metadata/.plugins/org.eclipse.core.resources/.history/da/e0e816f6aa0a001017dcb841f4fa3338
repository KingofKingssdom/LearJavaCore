package Arrays;

import java.util.Arrays;
import java.util.List;

public class Test {
	public static int phanTuLonThuHai (int [] array) {
		int n = array.length;
		int max = array[0];
		
		for(int i = 1; i < n;i++) {
			if(array[i -1] < array[i]) {
				max = array[i];
			}
		}
		
		int second = Integer.MAX_VALUE;
		for(int i = 1;i<n;i++) {
			if(array[i-1] < array[i] && array[i] != max) {
				second = array[i];
			}
		}
		return second;
	}
	public static void main(String[] args) {
		int [] array = {10,20,30,40,50,60,70};
		int result = phanTuLonThuHai(array);
		System.out.println(result);
	}
}
