package Arrays;

import java.util.Arrays;
import java.util.List;

public class Test {
	public static void inXenKe (int [] array) {
		for(int i = 0; i< array.length;i+=2) {
			System.out.println(array[i]);
		}
	}
	public static void main(String[] args) {
		int [] array = {10,20,30,40,50,60};
		inXenKe(array);
	}
}
